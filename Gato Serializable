import java.io.Serializable;

public class Gato implements Serializable {
    private String nombre;
    private int edad;

    public Gato(String nombre, int edad) {
        this.nombre = nombre;
        this.edad = edad;
    }

    public String getNombre() {
        return nombre;
    }

    public int getEdad() {
        return edad;
    }

    public String hacerSonido() {
        return "Miau";
    }
}
